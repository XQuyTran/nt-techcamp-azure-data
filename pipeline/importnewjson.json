{
	"name": "importnewjson",
	"properties": {
		"activities": [
			{
				"name": "c_copy_json_to_database",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"parallelCopies": 1,
					"dataIntegrationUnits": 2,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['sepID']"
								},
								"sink": {
									"name": "sepID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['eventTime']"
								},
								"sink": {
									"name": "eventTime",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['instruments'][0]['displayName']"
								},
								"sink": {
									"name": "instruments_displayName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['linkedEvents'][0]['activityID']"
								},
								"sink": {
									"name": "linkEvents_activityID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['link']"
								},
								"sink": {
									"name": "link",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_newjson",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_AzureSQLJSon",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2024-01-22T11:22:14Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}